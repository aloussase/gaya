let f1 = unit in
let f2 = unit in
let f3 = unit in 
let i = 1 in do
  while i < 4
    let j = i in do
      let f = { => j } in
      cases
        given j == 1 => &f1 <- f
        given j == 2 => &f2 <- f
        otherwise    => &f3 <- f
      end
    end.
    &i <- i + 1
  end

  f1() |> assert(_ == 1).
  f2() |> assert(_ == 2).
  f3() |> assert(_ == 3).
end.
